---
import { Schema } from "astro-seo-schema";
export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <Schema
      item={{
        "@context": "https://schema.org",
        "@type": "Person",
        name: "Gabriel Nascimento",
        url: "https://gabrielnascimento.netlify.app/",
        image: "",
        sameAs: [
          "https://www.linkedin.com/in/gabriel-jardim-nascimento/",
          "https://github.com/gabinpoa",
        ],
        jobTitle: "Desenvolvedor Web E React Native",
      }}
    />
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Urbanist:wght@300;400;500;600&family=VT323&display=swap"
      rel="stylesheet"
    />
  </head>
  <body class="min-h-screen text-white font-urbanist bg-slate-900">
    <slot />
  </body>
</html>
